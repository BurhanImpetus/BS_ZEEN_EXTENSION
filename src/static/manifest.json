{
  "name": "BS_ZEEN_Extension",
  "description": "BS Google Chrome Extension POC for ZEEN",
  "version": "1.0.0",
  "manifest_version": 3,
  "icons": {
    "16": "zeen_logo.png",
    "48": "zeen_logo.png",
    "128": "zeen_logo.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_title": "BS ZEEN Extension",
    "default_icon": "zeen_logo.png"
  },
  "permissions": ["storage", "tabs"],
  "options_page": "options.html",
  "background": {
    "service_worker": "background.js"
  },
  "web_accessible_resources": [
    {
      "resources": [ "*.png" ],
      "matches": [ "<all_urls>" ]
    }
  ],
  "content_scripts": [
    {
      "matches": ["https://*.jcrew.com/*"],
      "js": ["contentScript.js"],
      "run_at": "document_end"
    }
  ]
}
